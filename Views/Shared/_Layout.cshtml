<!DOCTYPE html>

<html>
<head>
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=11">
  <title>@ViewBag.Title</title>
  <link href="~/lib/myBulma.min.css" rel="stylesheet" />
  @*<link href="~/lib/fontawesome.min.css" rel="stylesheet" />*@
  <link href="~/lib/fontawesome/css/all.min.css" rel="stylesheet" />

  <link href="~/lib/Utilities.min.css" rel="stylesheet" />
  <link href="~/lib/main.min.css" rel="stylesheet" />
</head>
<body>

  <nav style="margin-bottom: 0;"
       class="navbar hide-for-print is-primary" role="navigation" aria-label="main navigation">
    <div class="container">
      <div class="navbar-brand">
        <a style="text-shadow: 2px 2px 0px rgba(90, 90, 90, 1);"
           class="navbar-item has-text-weight-bold is-size-4 has-text-white"
           href="/">
          Clay County Financial Services @ViewBag.Development
        </a>
      </div>
      <div class="navbar-menu is-active">
        <div class="navbar-start">
        </div>
        <div class="navbar-end">
        </div>
      </div>
    </div>
  </nav>


  <div>
    @RenderBody()
  </div>

   
</body>
</html>
